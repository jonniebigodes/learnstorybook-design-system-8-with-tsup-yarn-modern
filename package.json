{
  "name": "@jonniebigodes/learnstorybook-design-system-ts-yarn-modern",
  "description": "Storybook design systems tutorial changes for ts and yarn",
  "version": "0.1.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": "https://github.com/jonniebigodes/learnstorybook-design-system-8-with-tsup-yarn-modern",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "release": "yarn build && auto shipit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "build-storybook-docs": "storybook build  --docs"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@storybook/addon-a11y": "^8.5.8",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@storybook/test-runner": "^0.21.3",
    "@types/node": "18.15.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "auto": "^11.3.0",
    "chromatic": "^11.25.2",
    "concurrently": "8.0.1",
    "eslint": "8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.11.2",
    "polished": "^4.2.2",
    "prettier": "^3.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^8.5.8",
    "tsup": "7.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@4.5.0",
  "author": "jonniebigodes <testingemail@email.com>",
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
